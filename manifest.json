{
  "manifest_version": 2,
  "name": "regexFinder",
  "version": "0.1",
  "description": "Regex based CTRL+F",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title" : "Regex finder"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+H"
      }
    }
  },
  "permissions": ["tabs", "http://*/*", "https://*/*"]
}
